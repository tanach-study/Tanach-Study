{
  "name": "ts-react",
  "version": "1.0.0",
  "description": "A Gatsbyjs/React-based client app for the Tanach Study Program",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "build": "gatsby build",
    "clean": "rm -rf dist && rm -rf node_modules && rm -f package-lock.json && rm -rf .cache && rm -rf public",
    "deploy": "gatsby-plugin-s3 deploy",
    "develop": "NODE_ENV=development gatsby develop",
    "lint": "eslint './src/**/*.js' './src/**/*.jsx'",
    "lint:fix": "npm run lint -- --fix",
    "start": "npm run develop",
    "serve": "gatsby serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tanach-study/Tanach-Study.git"
  },
  "author": "Joey Pinhas",
  "license": "MIT",
  "engines": {
    "node": "v17.0.1",
    "npm": "8.1.0"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "gatsby": "^4.4.0",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-manifest": "^4.4.0",
    "gatsby-plugin-react-helmet": "^5.4.0",
    "gatsby-plugin-s3": "^0.3.8",
    "materialize-css": "^1.0.0",
    "node-fetch": "^3.1.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "eslint": "8.5.0",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.27.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/test/__mocks__/styleMock.js"
    }
  }
}
