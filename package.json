{
  "name": "ts-react",
  "version": "1.0.0",
  "description": "A Gatsbyjs/React-based client app for the Tanach Study Program",
  "main": "server.js",
  "scripts": {
    "build": "gatsby build",
    "clean": "rm -rf dist && rm -rf node_modules && rm -f package-lock.json && rm -rf .cache && rm -rf public",
    "deploy": "gatsby-plugin-s3 deploy",
    "develop": "NODE_ENV=development gatsby develop",
    "lint": "eslint './src/**/*.js' './src/**/*.jsx'",
    "lint:fix": "npm run lint -- --fix",
    "start": "npm run develop",
    "serve": "gatsby serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tanach-study/Tanach-Study.git"
  },
  "author": "Joey Pinhas",
  "license": "MIT",
  "engines": {
    "node": "v15.14.0",
    "npm": "v7.7.6"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "gatsby": "^2.24.10",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-manifest": "^2.4.20",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-s3": "^0.3.4",
    "materialize-css": "^1.0.0",
    "node-fetch": "^2.6.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "eslint": "7.5.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/test/__mocks__/styleMock.js"
    }
  }
}
