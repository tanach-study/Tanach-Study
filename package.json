{
  "name": "ts-react",
  "version": "1.0.0",
  "description": "A Gatsbyjs/React-based client app for the Tanach Study Program",
  "main": "server.js",
  "scripts": {
    "build": "gatsby build",
    "clean": "rm -rf dist && rm -rf node_modules && rm -f package-lock.json && rm -rf .cache && rm -rf public",
    "deploy": "gatsby-plugin-s3 deploy",
    "develop": "NODE_ENV=development gatsby develop",
    "lint": "eslint './src/**/*.js' './src/**/*.jsx'",
    "lint:fix": "npm run lint -- --fix",
    "start": "npm run develop",
    "serve": "gatsby serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tanach-study/Tanach-Study.git"
  },
  "author": "Joey Pinhas",
  "license": "MIT",
  "engines": {
    "node": "v20.0.0",
    "npm": "v9.6.4"
  },
  "dependencies": {
    "dotenv": "16.0.3",
    "gatsby": "5.9.0",
    "gatsby-plugin-google-fonts": "1.0.1",
    "gatsby-plugin-manifest": "5.9.0",
    "gatsby-plugin-react-helmet": "6.9.0",
    "gatsby-plugin-s3": "^0.3.2",
    "materialize-css": "1.0.0",
    "node-fetch": "2.6.6",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0"
  },
  "devDependencies": {
    "eslint": "8.39.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/test/__mocks__/styleMock.js"
    }
  }
}
